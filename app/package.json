{
  "name": "tew-indicators",
  "version": "1.0.0",
  "description": "'filter indicators for tracking equality at work'",
  "main": "src/app.js",
  "scripts": {
    "dev": "NODE_ENV=development npm run assets && npm run budo",
    "build": "browserify src/app.js | uglifyjs > build/bundle.js",
    "build-dev": "browserify src/app.js -o build/bundle.js -v",
    "assets": "cp src/index.html build/",
    "ready-deploy": "npm run build && npm run assets",
    "watch": "watchify src/app.js -o build/bundle.js -v",
    "budo": "budo src/app.js:bundle.js --dir build -v | garnish",
    "deploy-unix": "node gh-pages.js",
    "postinstall": "npm dedupe"
  },
  "browserify": {
    "transform": [
      "reactify",
      "brfs",
      "envify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dumparkltd/tew-indicators.git"
  },
  "keywords": [
    "indicators",
    "human-rights-commission"
  ],
  "author": "dumparkltd",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dumparkltd/tew-indicators/issues"
  },
  "homepage": "https://github.com/dumparkltd/tew-indicators",
  "dependencies": {
    "bootstrap": "^3.3.4",
    "capitalize": "^0.5.0",
    "debug": "^2.1.2",
    "domready": "^1.0.7",
    "fluxxor": "^1.5.2",
    "globals": "^6.4.1",
    "insert-css": "^0.2.0",
    "isarray": "0.0.1",
    "lodash": "^3.5.0",
    "lodash.clone": "^3.0.1",
    "lodash.contains": "^2.4.3",
    "lodash.filter": "^3.0.0",
    "lodash.find": "^3.2.0",
    "lodash.foreach": "^3.0.2",
    "lodash.matches": "^3.1.0",
    "lodash.omit": "^3.1.0",
    "lodash.pluck": "^3.0.2",
    "lodash.result": "^3.0.0",
    "lodash.uniq": "^3.1.0",
    "lodash.where": "^3.1.0",
    "promise": "^7.0.0",
    "qs": "^2.4.1",
    "react": "^0.13.1",
    "react-bootstrap": "^0.19.0",
    "react-router": "^0.13.1",
    "react-select": "^0.4.0",
    "reactify": "^1.0.0",
    "slug": "^0.9.1",
    "string.prototype.startswith": "^0.2.0",
    "tabletop": "git://github.com/timwis/tabletop",
    "to-slug-case": "^0.1.2",
    "url": "^0.10.3"
  },
  "devDependencies": {
    "brfs": "^1.4.0",
    "browserify": "^9.0.8",
    "budo": "^3.1.1",
    "colors": "^1.0.3",
    "envify": "^3.4.0",
    "garnish": "^2.1.3",
    "gh-pages": "^0.2.0",
    "watchify": "^2.6.2"
  }
}
