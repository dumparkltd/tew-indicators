{
  "name": "tew-indicators",
  "version": "1.0.0",
  "description": "'filter indicators for tracking equality at work'",
  "main": "src/app.js",
  "scripts": {
    "dev": "NODE_ENV=development npm run assets && npm run watch-css && npm run budo",
    "build": "browserify src/app.js | uglifyjs > build/bundle.js && npm run build-css",
    "build-dev": "browserify src/app.js -o build/bundle.js -d  && npm run build-css",
    "assets": "cp src/index.html build/",
    "build-css": "cat src/styles/*.css > build/bundle.css",
    "watch-css": "./node_modules/catw/bin/cmd.js src/styles/*.css -o build/bundle.css -v",
    "ready-deploy": "npm run build && npm run assets",
    "watch": "watchify src/app.js -o build/bundle.js -v -d && npm run watch-css",
    "budo": "budo src/app.js:bundle.js --dir build -v | garnish",
    "deploy-unix": "node gh-pages.js",
    "postinstall": "npm dedupe"
  },
  "browserify": {
    "transform": [
      "reactify",
      "brfs",
      "envify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dumparkltd/tew-indicators.git"
  },
  "keywords": [
    "indicators",
    "human-rights-commission"
  ],
  "author": "dumparkltd",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dumparkltd/tew-indicators/issues"
  },
  "homepage": "https://github.com/dumparkltd/tew-indicators",
  "dependencies": {
    "bootstrap": "^3.3.4",
    "capitalize": "^0.5.0",
    "d3-interpolate-rgb": "0.0.0",
    "debug": "^2.1.2",
    "fluxxor": "^1.5.2",
    "insert-css": "^0.2.0",
    "is-nan": "1.0.1",
    "isarray": "0.0.1",
    "lodash": "^3.5.0",
    "marked": "^0.3.3",
    "numeral": "^1.5.3",
    "ordinal": "0.0.2",
    "promise": "^7.0.0",
    "qs": "^2.4.1",
    "react": "^0.13.1",
    "react-bootstrap": "^0.19.0",
    "react-router": "^0.13.3",
    "react-select": "^0.4.0",
    "react-simple-table": "^0.6.0",
    "string.prototype.startswith": "^0.2.0",
    "tabletop": "git://github.com/timwis/tabletop",
    "to-slug-case": "^0.1.2",
    "traverse": "^0.6.6",
    "url": "^0.10.3",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "brfs": "^1.4.0",
    "browserify": "^9.0.8",
    "budo": "^3.1.1",
    "catw": "^1.0.1",
    "colors": "^1.0.3",
    "envify": "^3.4.0",
    "garnish": "^2.1.3",
    "gh-pages": "^0.2.0",
    "reactify": "^1.1.1",
    "watchify": "^2.6.2"
  }
}
